page{
  background: #f3f3f3;
}
.content{
  background: #ffffff;
  font-size: 14px;
  margin-bottom: 5px;
}
.same{
  border-bottom: 1px solid #eaeaea;
  height: 40px;
  border-top: 1px solid #eaeaea;
}
.same text{
  display: block;
  float: left;
  line-height: 40px;
  width: 20%;
  padding-left: 10px;
}
.same input{
  display: block;
  float: left;
  height: 40px;
  width: 70%;
}
.list:nth-child(1){
 border-top: 1px solid #cccccc;
}
.list text{
  font-size: 12px;
  line-height: 30px;
  padding-left: 10px;
}
.address{
  float: right;
  width: 70%;
}
.address text{
  width: 80%;
  float: right;
  text-align: right;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.address image{
  width: 12rpx;
  height: 21rpx;
  margin-top: 29rpx;
  float: right;
  margin-left: 2%;
  margin-right: 2%;
}
.same icon{
  margin-top: 10px;
  float: right;
  margin-right: 10px;
}
.btn{
  font-size: 14px;
  background: #010101;
  color: #fff;
  width: 690rpx;
  height: 80rpx;
  border-radius: 0;
  padding: 0;
  line-height: 80rpx;
  margin-top: 80rpx;
}
button::after{
  content: none;
}
/*省市联动样式*/
.citypicker{
  position: fixed;
  height: 220px;
  width: 100%;
  min-height: 100%;
  background: #fff;
  z-index: 1000;
}
.citybody {
  position: fixed;
  bottom: 0px;
}
.cityheader {
  position: absolute;
  top:0px;
  width: 100%;
  z-index: 111;
  background: #fff;
}
.city-cancel {
  float: left;
  margin: 20rpx;
  color: #818181;
  font-size: 12px;
}
.city-true {
  font-size: 12px;
  float: right;
  margin: 20rpx;
  color: #2FB42E
}
.addressInfo{
  font-size: 12px;
  line-height: 35px;
  text-align: center;
}
