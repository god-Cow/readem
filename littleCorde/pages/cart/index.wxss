.carts{
  border-top: 1px solid #eaeaea;
  border-bottom: 1px solid #eaeaea;
}
.carts .cartList {
  overflow: hidden;
  padding-top: 10px;
  padding-bottom: 10px;
  height: 80px;
  border-top:1px solid #eaeaea;
}
.carts .cartList:nth-child(1){
  border-top: 0;
}
.floatLeft {
  float: left;
}
.carts .checked {
  width: 40px;
  margin-top: 30px;
}
.carts icon {
  float: left;
  margin-left: 10px;
}
.carts navigator {
  float: left;
  width: 80px;
  height: 80px;
  margin-left: 10px;
}
.carts navigator image {
  width: 100%;
  height: 100%;
}
.carts .shopInfo {
  margin-left: 20px;
}
.carts .shopInfo text {
  font-size: 22rpx;
  line-height: 20px;
  height: 20px;
  display: block;
  width: 165px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  color: #010101;
}
.carts .shopInfo text:nth-of-type(1) {
  font-weight: bolder;
}
.shopHandle {
  float: left;
  position: absolute;
  right: 10px;
  bottom: -5px;
}
.shopHandle text {
  float: left;
  font-size: 12px;
  line-height: 50rpx;
  padding-left: 5px;
  padding-right: 5px;
}
.min {
  width: 50rpx;
  height: 50rpx;
  float: left;
}
.max {
  width: 50rpx;
  height: 50rpx;
}
.orderAmount {
  border-bottom: 1px solid #eaeaea;
  overflow: hidden;
}
.orderAmount view {
  display: inline-block;
  font-size: 14px;
  line-height: 30px;
}
.left {
  float: left;
  margin-left: 15px;
}
.right {
  float: right;
  margin-right: 15px;
}
.cartNoNull {
  padding-bottom: 36px;
  width: 100%;
  overflow: hidden;
}
.cartNull {
  border-top: 1px solid #eaeaea;
  font-size: 14px;
  text-align: center;
  padding-top: 150px;
}
.del {
  background: #000;
  width: 60px;
  height: 100px;
  color: #fff;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  -webkit-transform: translateX(65px);
  transform: translateX(65px);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  text-align: center;
  line-height: 100px;
  font-size: 14px;
  display: block;
  float: left;
  margin-top: -10px;
}
.content {
  width: 100%;
  margin-right: 0;
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  -webkit-transform: translateX(65px);
  transform: translateX(65px);
  margin-left: -65px;
  position: relative;
  overflow: hidden;
  float: left;
}
.newActive .content, .newActive .del {
  -webkit-transform: translateX(5px);
  transform: translateX(5px);
}
.tips {
  border-top: 1px solid #eaeaea;
  display: flex;
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 80rpx;
  background: #fff;
}
.tips .lxCheckboxGroup {
  height: 80rpx;
  flex: 1;
}
.lxCheckboxGroup view {
  margin: 9px auto;
}
.lxCheckboxGroup icon {
  float: left;
  margin-left: 10px;
  margin-right: 5px;
}
.lxCheckboxGroup text {
  font-size: 14px;
  height: 23px;
  line-height: 23px;
  display: block;
}
.tips .btn {
  flex: 1;
}
.btn button {
  width: 100%;
  margin: 0 auto;
  background: #000;
  color: #fff;
  font-size: 14px;
  height: 80rpx;
  line-height: 80rpx;
  border: 0;
  border-radius: 0;
}
.cartTitle {
  width: 100%;
}
.classifyList {
  font-size: 30rpx;
  line-height: 80rpx;
  font-weight: bolder;
  width: 100%;
  background: #efeff4;
  padding-left: 10px;
  color: #010101;
  height: 80rpx;
}
.activeName {
  font-size: 24rpx;
  line-height: 40px;
  padding-left: 10px;
  height: 40px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-top: 2px;
}
.line {
  border: 1px solid #999;
  padding: 0 2px;
}
.activeDis {
  margin-left: 10px;
  font-size: 24rpx;
}
.newBody {
  width: 100%;
  background: #000;
  opacity: 0.5;
  position: fixed;
  top: 0;
  left: 0;
  height: 100%;
}
.checkShop {
  background: #fff;
  z-index: 400;
  position: fixed;
  width: 80%;
  height: 350rpx;
  top: 50%;
  left: 10%;
  margin-top: -175rpx;
}
.newTitle {
  font-size: 28rpx;
  line-height: 60rpx;
  height: 60rpx;
  margin: 10rpx auto;
  text-align: center;
  border-bottom: 1px solid #eaeaea;
}
.shopClassify {
  height: 200rpx;
  margin-bottom: 20rpx;
}
.shopClassifyList {
  height: 100rpx;
  overflow: hidden;
  margin-left: 10px;
  line-height: 100rpx;
  margin-bottom: 10rpx;
  border-top: 1px solid #eaeaea;
}
.shopClassifyList:nth-of-type(1){
  border-top:0;
}
.shopClassifyList .checked icon {
  margin-top: 10rpx;
}
.css {
  height: 100rpx;
  margin-left: 10px;
  float: left;
}
.checkTitle {
  line-height: 50rpx;
  font-size: 12px;
}
.checkCount {
  line-height: 50rpx;
  font-size: 12px;
  color: #999;
}
.checkBtn {
  height: 80rpx;
  width: 100%;
  overflow: hidden;
  border-top: 1px solid #eaeaea;
}
.checkBtn view {
  width: 50%;
  float: left;
  height: 80rpx;
  font-size: 12px;
  text-align: center;
  line-height: 80rpx;
}
.checkBtn .backCart {
  background: #fff;
}
.gopay {
  background: #000;
  color: #fff;
}
.sharePrice{
  position: absolute;
  left: 100rpx;
  bottom: 5rpx;
  font-size: 18rpx!important;
  background: #010101;
  color: #fff!important;
  width: 120rpx!important;
  height: 30rpx!important;
  line-height: 30rpx!important;
  text-align: center;
}