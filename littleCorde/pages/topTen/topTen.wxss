.content {
  width: 100%;
  overflow: hidden;
}
.list {
  width: 690rpx;
  margin: 30rpx;
  overflow: hidden;
}
.goods {
  width: 335rpx;
  height: 440rpx;
  border: 1rpx solid #dcdcdc;
  margin: 0 20rpx 20rpx 0;
  float: left;
  box-sizing: border-box;
}
.goods:nth-child(2n) {
  margin-right: 0;
}
.goodsimg {
  width: 250rpx;
  height: 250rpx;
  margin: 25rpx auto;
  display: block;
}
.brand {
  font-size: 22rpx;
  margin-bottom: 12rpx;
}
.brand, .designer, .salePrice {
  display: block;
  width: 90%;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin: 0 auto 12rpx; 
}
.designer {
  display: block;
  font-size: 24rpx;
  margin-bottom: 12rpx;
  text-align: center;
}
.salePrice {
  display: block;
  font-size: 24rpx;
  margin-bottom: 12rpx;
  text-align: center;
}
.last {
  margin: 50rpx auto;
  width: 235rpx;
  height: 62rpx;
}
.last image {
  width: 235rpx;
  height: 62rpx;
}
