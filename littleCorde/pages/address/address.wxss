page{
  background: #e9e9e9;
}
.address text{
  font-size: 14px;
  text-align: center;
  line-height: 200px;
  display: block;
}
.addAddress{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 50px;
  background: #000;
  color: #fff;
  overflow: hidden;
}
.addAddress navigator{
  width: 50%;
  height: 50px;
  display: block;
  float: left;
}
.addAddress navigator text{
  text-align: center;
  line-height: 50px;
  font-size: 28rpx;
  display: block;
}
.addAddress button{
  width: 50%;
  height: 50px;
  display: block;
  float: left;
  background: #ccc;
  font-size: 28rpx;
  line-height: 50px;
  color: #fff;
}
.addressList{
  background: #ffffff;
  border-top: 1px solid #eaeaea;
  width: 100%;
  margin-bottom: 50px;
}
.list{
  padding-left: 10px;
  padding-top: 10px;
  padding-bottom: 10px;
  height: 60px;
  border-bottom: 1px solid #eaeaea;
  overflow: hidden;
}
.addressInfo{
  flex: 6;
  font-size: 12px;
  line-height: 20px;
}
.name{
  font-size: 14px;
}
.defaultAddress text{
  font-size: 12px;
  display: block;
  line-height: 60px;
  text-align: center;
}
.newDefaultAddress{
  flex: 1;
}
.newDefaultAddress text{
  display: none;
}
.defaultAddress , .right{
  flex: 1;
}
.right image{
  width:12rpx;
  height:21rpx;
  display: block;
  margin: 50rpx auto;
}
 .content{
  width: 100%;
  margin-right:0;
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  -webkit-transform: translateX(120px);
  transform: translateX(120px);
  margin-left: -120px; 
  position: relative;
  overflow: hidden;
  float: left;
  display: flex;
}
.newActive .content, .newActive .operate{
  -webkit-transform: translateX(0);
  transform: translateX(0);
}
.operate{
  background: #ccc;
  width: 120px;
  height:80px;
  color: #ffffff;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  -webkit-transform: translateX(120px);
  transform: translateX(120px);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  text-align: center;
  line-height: 80px;
  font-size: 14px;
  display: block;
  float: left;
  margin-top: -10px;
}
.del{
  background: #000000;
}
.del , .redact{
  width: 60px;
  height: 80px;
  display: inline-block;
}