.moreSize .gray .grayList{
  overflow: hidden;
}
.moreSize .grayChecked{
  background: #010101!important;
  color: #fff;
} 
.stockCheck{
  background: #EBEBEB;
  color:#B6B6B6;
} 
.cartBtn{
  height: 100rpx;
  font-size: 14px;
  line-height: 100rpx;
} 
button::after{
  content:none;
}
button[type="default"] {
  background: #000;
  color: #fff;
}
.newBody{
  width: 100%;
  background: #000;
  opacity: 0.6;
  position: fixed;
  top: 0;
  left: 0;
  height: 100%;
  z-index: 50;
}

.collage_group{
  background: #fff;
  width: 100%;
  height: 370rpx;
  margin-top: 20rpx;
}
.collage_group .group_title{
  font-size: 28rpx;
  padding-left: 30rpx;
  padding-top:40rpx;
}
.collage_group .group_title .deep{
  font-weight: bold;
  display: block;
  text-align: center;
  margin-bottom: 20rpx;
  color: #1d1d1d;
}
.collage_group .group_title .deep_color{
  color:#f6ab30;
  padding: 0 10rpx;
}
.collage_group .group_title .noColor{
  margin-left: 20rpx;
}
.collage_group .group{
  width: 650rpx;
  margin: 40rpx auto 0;
  display: flex;
  justify-content:space-between;
}
.collage_group .group view{
  width: 96rpx;
  height: 96rpx;
}
.collage_group .group .loader{
  position: relative;
}
.collage_group .group .loader text{ 
  font-size: 20rpx;
  color: #fff;
  background: rgb(245,166,35);
  padding: 3rpx 8rpx;
  position: absolute;
  bottom: -11rpx;
  left: 50%;
  margin-left: -28rpx;
}
.collage_group .group image{
  width: 96rpx;
  height: 96rpx;
  border-radius: 50%;
  margin: 0 auto;
  display: block;
}
.tips{
  width: 690rpx;
  font-size: 24rpx;
  margin: 40rpx auto 0;
  text-align: center;
}
.imgList{
  width: 100%;
}
.itemImg{
  width: 750rpx;
  margin: 0 auto;
}
.itemImg image{
  width: 100%;
  display: block;
}
.oldTipsBg{
  position: fixed;
  width: 100%;
  height: 100%;
  z-index: 99;
  top: 0;
  left: 0;
  background: rgba(0,0,0,0.6);
}
.oldTips{
  width: 600rpx;
  height: 360rpx;
  background: #fff;
  position: fixed;
  top: 50%;
  left: 50%;
  margin-left: -300rpx;
  margin-top: -180rpx;
  z-index: 100;
}
.oldTitle{
  text-align: center;
  font-size: 32rpx;
  color: rgb(29,29,29)
}
.oldTips .oldTitle:nth-of-type(1){
  margin-top: 80rpx;
}
.oldTips button{
  display: block;
  width: 280rpx;
  height: 80rpx;
  background: rgb(29,29,29);
  font-size: 28rpx;
  color: #fff;
  text-align: center;
  line-height: 80rpx;
  margin: 40rpx auto;
}
.blackActive{
  color: #fff!important;
  background: #000!important;
}
.goIndex {
  position: fixed;
  bottom: 110px;
  right: 20px;
  display: block;
  width: 100rpx;
  height: 100rpx;
  z-index: 10;
  background: url("https://img3.cloudokids.cn/h5img/scene/cloudoIcon2.png") no-repeat;
  background-size: 100% 100%;
}
.infoTips{
  font-size: 28rpx;
  height: 64rpx;
  line-height: 64rpx;
  border-radius: 32rpx;
  background: #f5a623 url('https://img3.cloudokids.cn/h5img/scene/info@2x.png') no-repeat 18rpx center;
  background-size: 28rpx 28rpx;
  position: fixed;
  bottom:65px;
  right: 10px;
  z-index: 10;
  width: 290rpx;
  padding-left: 60rpx;
  color: #fff;
}
.infoTips1{
  width: 0;
  height: 0;
  border-width: 6px;
  border-style: solid;
  border-color: #f5a623 transparent transparent transparent;
  position: fixed;
  bottom:53px;
  right: 55px;
  z-index: 10;
}
.barrage{
  position: absolute;
  top: 30rpx;
  left: 20rpx;
  background: rgba(0,0,0,0.6);
  border-radius: 34rpx;
  height: 68rpx;
  width: 550rpx;
  line-height: 68rpx;
  z-index: 10;
}
.barrage image{
  width: 60rpx;
  height: 60rpx;
  display: block;
  margin: 4rpx;
  float: left;
  border-radius: 50%;
}
.barrage view text{
  font-size: 24rpx;
  color: #fff;
}
.barrage view text:nth-of-type(1){
  color: #f5a623;
}
.barrage view text:nth-of-type(2){
  color: #fff;
}