@import "/assets/css/size.wxss";
page{
  background: #f7f7f7;
}
.content{
  width: 100%;
  background: #fff;
}
.swiper-box{
  width: 100%;
  height: 648rpx;
}
.swiper-box swiper-item{
  width: 100%;
  height: 648rpx; 
}
.swiper-box swiper-item image{
  display: block;
  width: 648rpx;
  height: 648rpx;
  margin: 0 auto;
}
.swiper .wx-swiper-dots.wx-swiper-dots-horizontal {
  margin-bottom: 10px;
}
.country{
  width: 142rpx;
  margin: 40rpx auto;
  text-align: center;
  height: 50rpx;
  overflow: hidden;
}
.country image{
  width: 36rpx;
  height: 27rpx;
  display: block;
  margin: 13.5rpx auto 0;
  float: left;
}
.country text{
  font-size: 24rpx;
  line-height: 50rpx;
  display: block;
  float: left;
  margin-left: 10rpx;
  color: rgb(155,155,155)
}
.goodsName{
  width: 90%;
  margin: 0 auto 10rpx;
  text-align: center;
  font-size: 32rpx;
  color: rgb(29,29,29);
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.discription{
  font-size: 24rpx;
  color: rgb(155,155,155);
  width: 90%;
  margin: 0 auto 15rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-align: center;
}
.all{
  width: 100%;
  height: 70rpx;
  text-align: center;
  margin: 0 auto 15rpx;
}
.all text{
  display: inline-block;
}
.all text:nth-of-type(1){
  font-size: 44rpx;
  color: rgb(29,29,29);
  position: relative;
  top: 8rpx;
}
.all text:nth-of-type(2){
  font-size: 28rpx;
  color: rgb(155,155,155);
  margin-left: 20rpx;
  position: relative;
  top: 5rpx;
}
.all text:nth-of-type(3) , .all text:nth-of-type(4) {
  font-size: 24rpx;
  color: rgb(245,166,35);
  border: 1px solid rgb(245,166,35);
  padding: 2rpx 8rpx;
  margin-left: 20rpx;
}
.number{
  text-align: center;
  font-size: 24rpx;
  color: rgb(102,102,102);
  padding-bottom: 40rpx;
}
.same{
  width: 100%;
  margin: 20rpx auto;
  background: #fff;
  padding-bottom: 40rpx;
}
.title{
  font-size: 28rpx;
  margin: 0rpx auto 0 30rpx;
  padding-top: 40rpx;
  text-align: center;
  color: #1d1d1d;
  font-weight: bold;
}
.same image{
  width: 100%;
  height: 216rpx;
}
.list{
  width: 100%;
}
.listSame{
  width: 690rpx;
  margin: 0 auto 16rpx;
  overflow: hidden;
}
.dot{
  float: left;
  width: 12rpx;
  height: 12rpx;
  border-radius: 50%;
  background: #666;
  margin-right: 16rpx;
  margin-top: 10rpx;
}
.listSame .text{
  color: #666;
  font-size: 24rpx;
  float: left;
}
.footer{
  height: 50px;
  width: 100%;
  display: flex;
  background: #fff;
}
.service{
  flex: 1;
  background:url("https://img3.cloudokids.cn/h5img/icon/goService.png") no-repeat center 10rpx;
  background-size:40rpx 40rpx;
  font-size: 24rpx;
  line-height: 75px;
  text-align: center;
}
.goodsSize{
  flex: 1;
  background:url("http://img1.cloudokids.cn/buyers/img/weiXinchicun@3x.png") no-repeat center 10rpx;
  background-size:40rpx 40rpx;
  font-size: 24rpx;
  line-height: 75px;
  text-align: center;
}
.shopping{
  height: 50px;
  flex: 2;
  border-radius: 0;
  background: #000;
  color: #fff;
  text-align: center;
  font-size: 26rpx;
  border: none;
  outline: none;
}
.shopping text{
  display: block;
  height: 25px;
  line-height: 25px;
}
.shopping text:nth-of-type(1){
  line-height: 30px;
}
.colorGray{
  background: #535353!important;
}
.pictureShare{
  position: fixed;
  right: 20px;
  background: url("https://img3.cloudokids.cn/h5img/scene/cloudoIcon1.png") no-repeat;
  background-size: 100% 100%;
  display: block;
  width: 100rpx;
  height: 100rpx;
  z-index: 10;
  bottom: 170px;
}
.item {
  font-size: 28rpx;
  height: 50px;
  padding: 0 !important;
  line-height: 50px !important;
}
.cancel{
  font-size: 28rpx;
  line-height: 30px;
}

.item button {
  background: #fff !important;
  height: 50px !important;
  line-height: 50px !important;
  font-size: 28rpx !important;
}
.item button::after {
  content: none;
}
