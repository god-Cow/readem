page{
  background: #f7f7f7;
} 
.pay_sucess{
  background: #fff;
  padding: 60rpx 0;
  border-bottom: 20rpx solid #f7f7f7;
}
.pay_sucess image{
  display: block;
  width: 76rpx;
  height: 76rpx;
  margin: 0 auto;
}
.pay_sucess .pay_ok{
  font-size: 32rpx;
  color: rgb(29,29,29);
  text-align: center;
  margin-top:20rpx;
}
.pay_sucess .pay_look{
  font-size: 24rpx;
  color: rgb(68,68,68);
  text-align: center;
  margin-top:20rpx;
}
/* 拼团中 */
.collage_group{
  background: #fff;
  width: 100%;
  /* height: 255rpx; */
  border-bottom: 20rpx solid #f7f7f7;
}
.collage_group .group_title{
  font-size: 28rpx;
  text-align: center;
  padding-top:20rpx;
}
.collage_group .deep{
  font-weight: bold;
  font-size: 28rpx;
  text-align: center;
  display: block;
  margin-bottom: 20rpx;
}
.collage_group .group_title .deep_color{
  color:#f6ab30;
  padding: 0 10rpx;
}
.collage_group .group_title .noColor{
  margin-left: 20rpx;
}
.collage_group .group{
  width: 650rpx;
  margin: 40rpx auto 50rpx  ;
  display: flex;
  justify-content:space-between;
}
.collage_group .tipimg{
  font-size: 28rpx;
  text-align: center;
  margin-bottom: 40rpx;
}
.collage_group .group view{
  width: 96rpx;
  height: 96rpx;
}
.collage_group .group .loader{
  position: relative;
}
.collage_group .group .loader text{ 
  font-size: 20rpx;
  color: #fff;
  background: rgb(245,166,35);
  padding: 3rpx 8rpx;
  position: absolute;
  bottom: -11rpx;
  left: 50%;
  margin-left: -28rpx;
}
.collage_group .group image{
  width: 96rpx;
  height: 96rpx;
  border-radius: 50%;
  display: block;
  margin: 0 auto;
}
/* 拼团中结束 */
.tips{
  margin-top: 160rpx;
}
.tips view{
  text-align: center;
  font-size: 28rpx;
  color: rgb(155,155,155);
}

.bottom_btn{
  height: 108rpx;
  width: 100%;
  position: fixed;
  bottom: 0;
  left: 0;
}
.bottom_btn view{
  float: left;
  width: 50%;
  height: 108rpx;
  line-height: 108rpx;
  text-align: center;
  font-size: 32rpx;
  color: #fff;
}
.bottom_btn .mp{
  background: #666666;
}
.bottom_btn .mp button{
  background: none;
  margin-top: 12rpx;
  font-size: 32rpx;
  color: #fff;
}
.bottom_btn .mp image{
  width: 48rpx;
  height: 40rpx;
}
.bottom_btn .friend{
  background: #333333;
}
.bottom_btn .friend image{
  width: 42rpx;
  height: 44rpx;
}
.bottom_btn image{
  color: #fff;
  position: relative;
  top:12rpx;
}
.tooltipBg{
  background: rgba(0,0,0,0.6);
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
}
.tooltip{
  width: 580rpx;
  height: 640rpx;
  background: #fff;
  position: fixed;
  top: 50%;
  left: 50%;
  margin-top: -320rpx;
  margin-left: -290rpx;
  z-index: 20;
}
.tooltip .same{
  width: 380rpx;
  height: 128rpx;
  margin: 20rpx auto 0;
  display: block;
}
.tooltip .same image{
  width: 100%;
  height: 100%;
}
.tooltip .title{
  font-size: 28rpx;
  text-align: center;
  color: #444444;
  margin-top: 120rpx;
}
.etitle{
  font-size: 28rpx;
  text-align: center;
  color: #444444;
  margin-bottom: 50rpx;
}
.cloudoImg{
  width: 50rpx;
  height: 50rpx;
  position: fixed;
  top: 50%;
  left: 50%;
  margin-top: -280rpx;
  margin-left: 185rpx;
  z-index: 20;
}
.site{
  margin-top:112rpx; 
}
.siteList{
  width: 100%;
  margin-top: 20rpx;
  background: #fff;
}
.doc{
  width: 690rpx;
  margin: 0 auto;
  padding-bottom: 80rpx;
  position: relative;
   background: url("https://img3.cloudokids.cn/h5img/scene/cloudo-index-bg.png") no-repeat 0 34rpx;
  background-size: 64rpx 88rpx;
}
.doc .title{
  overflow: hidden;
  box-sizing: border-box;
  height: 150rpx;
  padding: 30rpx 0;
}
.left{
  float: left
}
.top{
  font-size: 16rpx;
  color: #333;
}
.index{
  font-size: 28rpx;
  color: #333;
  width: 60rpx;
  text-align: center;
}
.index.left{margin-top: 20rpx;}
.line{
  margin-left: 20rpx;
  width: 2rpx;
  height: 55rpx;
  background: #dedede;
}
.activityDescription{
  color: #333;
  font-size: 36rpx;
  margin-left: 20rpx;
  width: 600rpx;
  overflow: hidden;
  display: flex;
  display: -webkit-flex;
  flex-direction: column;
}
.activityDescription .title{
  height: 50rpx; line-height: 50rpx; white-space: nowrap; overflow: hidden;text-overflow: ellipsis; padding: 0;
  color: #333;font-size: 36rpx;
}
.activityDescription .desc{
  font-size: 28rpx; color:#777;
  white-space: nowrap; overflow: hidden;text-overflow: ellipsis; line-height: 40rpx;
}

.siteImg{
  width: 100%;
  display: block;
}
.getUp{
  position: absolute;
  bottom: 35rpx;
  left: 50%;
  display: block;
  width: 360rpx;
  height: 96rpx;
  border-radius: 48rpx;
  box-shadow: 0 8rpx -12rpx rgba(255, 54, 54, 0.3) outset;
  margin-left: -180rpx;
  text-align: center;
  box-sizing: border-box; padding: 10rpx 0; line-height:30rpx;
  vertical-align: middle;
}
.color3{
  background: linear-gradient(to right, #ff488e, #ffbf84);
}
.color4{
  background: linear-gradient(to right, #858585, #d4d4d4);
}
.getUp text{
  display: block;
}
.getUp text:nth-of-type(1){
  color: #fff;
  font-size: 26rpx;
  line-height:40rpx;
}
.getUp text:nth-of-type(2){
  color: rgba(255,255,255,0.7);
  font-size: 28rpx;
}