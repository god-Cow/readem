.swiper-tab{ 
  width: 100%; 
  border-bottom: 10px solid #f3f3f3;
  border-top: 1px solid #eaeaea; 
  text-align: center; 
  line-height: 80rpx;
  } 
.swiper-tab-item{ 
  font-size: 30rpx; 
  display: inline-block; 
  width: 25%; 
  color: #666666; 
}
.swiper-tab-item text{
  display: block;
}
.swiper-tab-item image{
  width: 50px;
  height: 50px;
}
.on{  
  border-bottom: 1rpx solid #000;
} 
.swiper-box{ 
  display: block; 
  height: 100%; 
  width: 100%; 
  overflow: hidden; 
} 
.orderList{
  overflow: hidden;
  font-size: 14px;
  height: 40px;
  padding: 10px 0;
  border-bottom: 1px solid #eaeaea;
}
.orderNumber{
  float: left;
  width: 80%;
  line-height: 20px;
}
.orderNumber text{
  display: block;
  padding-left: 10px;
}
.orderNumber text:nth-of-type(2){
  font-size: 12px;
}
.orderSame{
  float: left;
  width: 20%;
  line-height: 40px;
  position: relative;
}
.orderSame image{
  width: 12rpx;
  height: 20rpx;
  padding-top: 30rpx;
  position: absolute;
  top: 0;
  left: 55px;
}