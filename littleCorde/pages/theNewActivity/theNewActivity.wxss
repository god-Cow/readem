page{
  height: 100%;
  box-sizing: border-box;
}
.bannerImg{
  width: 750rpx;
  height: 1207rpx;
  margin: 0 auto;
}
.bannerImg image{
  display: block;
  width: 100%;
  height: 100%;
}
.title{
  text-align: center;
  font-size: 36rpx;
  margin: 90rpx auto 10rpx;
  font-weight: 600;
}
.etitle{
  font-size: 23rpx;
  text-align: center;
  margin: 0 auto 50rpx;
}
.list{
  width: 700rpx;
  overflow: hidden;
  margin: 50rpx auto 75rpx;
}
.brand1{
  display: block;
  width: 344rpx;
  height: 458rpx;
  margin-right: 12rpx;
  margin-bottom: 35rpx;
  float: left;
}
.brand1:nth-child(2n){
  margin-right:0;
}
.brand1 image{
  width: 100%;
  height: 100%;
}
.list2{
  width: 720rpx;
  margin-left: 30rpx;
  height: 246rpx;
  overflow: hidden;
  white-space:nowrap;
}
.brand2{
  width: 334rpx;
  height: 246rpx;
  margin-right: 12rpx;
  display: inline-block;
}
.brand2 image{
  width: 100%;
  height: 100%;
}
.news{
  width: 700rpx;
  overflow: hidden;
  margin: 0 auto 75rpx;
}
.brand3{
  width: 344rpx;
  height: 460rpx;
  float: left;
  margin-right: 12rpx;
  margin-bottom: 35rpx;
}
.brand3:nth-child(2n){
  margin-right: 0;
}
.brand3 image{
  width: 100%;
  height: 100%;
}
.brand4{
  display: block;
  width: 344rpx;
  height: 212rpx;
  margin-right: 12rpx;
  float: left;
  margin-bottom: 10rpx;
}
.brand4:nth-child(2n){
  margin-right: 0
}
.brand4 image{
  width: 100%;
  height: 100%;
}
.lasta{
  display: table;
  font-size:24rpx;
  padding: 8rpx 50rpx;
  border: 1px solid #000;
  margin: 100rpx auto;
}
.share{
  background: url("https://img3.cloudokids.cn/h5img/scene/cloudoIcon1.png") no-repeat;
  background-size:100% 100%;
  width: 90rpx;
  height: 90rpx;
  position: fixed;
  bottom: 165rpx;
  right: 25rpx;
}
.goIndex{
  background: url("https://img3.cloudokids.cn/h5img/scene/cloudoIcon2.png") no-repeat;
  background-size:100% 100%;
  width: 90rpx;
  height: 90rpx;
  position: fixed;
  bottom: 40rpx;
  right: 25rpx;
}
.item{
  font-size: 28rpx;
  height: 39px;
  padding: 0!important;
  line-height: 39px!important;
}
.cancel{
  font-size: 28rpx;
}
.item button{
  background: #fff!important;
  height: 39px!important;
  line-height: 39px!important;
  font-size: 28rpx!important;
}
.item button::after{
  content: none;
}
.canvasBg{
  position: fixed;
  height: 100%;
  width: 100%;
  left: 0;
  top: 0;
  z-index: 100;
  background: rgba(0,0,0,0.5);
}
.spuCanvas{
  position: fixed;
  width: 280px;
  height: 490px;
  left: 50%;
  top: 50%;
  z-index: 111;
  margin-top: -240px;
  margin-left: -145px;
}
.spuCanvasImg{
  width: 280px;
  height: 430px;
}
.spuCanvas view{
  height: 60px;
  width: 280px;
  overflow: hidden;
}
.spuCanvas view button{
  border: 0;
  border-radius: 0;
  height: 40px;
  width: 220rpx;
  font-size: 24rpx;
  line-height: 40px;
  margin-top: 20px;
}
.spuCanvas view button::after{
  content: none
}
.spuCanvas view .btn1{
  background: #010101;
  color: #fff;
  float: left;
}
.spuCanvas view .btn2{
  background: #666;
  color: #fff;
  float: right;
}
.navigation{
  width: 90rpx;
  height: 90rpx;
  position: fixed;
  right: 25rpx;
  bottom: 290rpx;
}
.navigation image{
  display: block;
  width: 100%;
  height: 100%;
}
.navigationList{
  width: 221rpx;
  height: 507rpx;
  position: fixed;
  right: 0;
  top: 200rpx;
  background: url("https://img3.cloudokids.cn/h5img/newBuyers/newBuyersImg/the2018newactivity62.png") no-repeat;
  background-size: 100% 100%;
}
.navigationNone{
  width: 0;
  transition:width 0.8s;
  -webkit-transition:width 0.8s;
}
.navigationYes{
  width: 221rpx;
  transition:width 0.8s;
  -webkit-transition:width 0.8s;
}
.navigationListSame{
  width: 221rpx;
  height: 65rpx;
}
.navigationListSame:nth-child(1){
  margin-top: 23rpx;
}
.gameOver{
  text-align: center;
  line-height: 600rpx;
  font-size: 26rpx;
}
.brand5{
  display: block;
  width: 700rpx;
  height: 590rpx;
  margin: 0 auto;
}
.brand5 image{
  width: 100%;
  height: 520rpx;
}
.brand5 swiper-item navigator{
  width: 100%;
  height: 520rpx;
}
.swiper .wx-swiper-dots.wx-swiper-dots-horizontal {
  margin-bottom: 0rpx;
}