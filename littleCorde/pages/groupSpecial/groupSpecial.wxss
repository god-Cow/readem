@import "../newStyle/newStyle.wxss";
.noGroupSamll{
  height: 75rpx;
  background: #cc171d;
  width: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 5000;
  color: #fff;
  font-size: 24rpx;
  line-height: 75rpx;
  text-align: center;
  overflow: hidden;
  letter-spacing: 2rpx;
}
.noGroupbig{
  width: 100%;
  height: 0;
  position: fixed;
  z-index: 5000;
  top: 0;
  left: 0;
  overflow: hidden;
}
.noGroupbig view{
  height: 725rpx;
  width: 100%;
  position: relative;
}
.noGroupbig1{
  height: 0;
  transition: height 1s;
  -webkit-transition: height 1s;
  background: rgba(0,0,0,0.3);
}
.noGroupbig2{
  height: 100%;
  transition: height 1s;
  -webkit-transition: height 1s;
  background: rgba(0,0,0,0.3);
}
.noGroupbig view image{
  width: 100%;
  height: 725rpx;
  display: block;
}
.noGroupbig view button{
  height: 75rpx;
  width: 272rpx;
  background: #cc171d;
  font-size: 24rpx;
  line-height: 75rpx;
  color: #fff;
  border-radius: 35rpx;
  position: absolute;
  bottom: 100rpx;
  left: 50%;
  margin-left: -136rpx;
}
.noGroupbig view .rule{
  display: block;
  font-size: 20rpx;
  color: #a0a0a0;
  text-align: center;
  position: absolute;
  left: 0;
  width: 100%;
  bottom: 45rpx;
}
.noGroupbig view .description{
  display: block;
  font-size: 24rpx;
  color: #fff;
  text-align: center;
  position: absolute;
  left: 0;
  width: 100%;
  bottom: 315rpx;
}
.noGroupbig view .skipUrl{
  display: block;
  font-size: 30rpx;
  color: #fff;
  text-align: center;
  position: absolute;
  left: 0;
  width: 100%;
  bottom: 265rpx;
}
.yesGroupSamll{
  height: 75rpx;
  background: #cc171d;
  width: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 5000;
  color: #fff;
  font-size: 24rpx;
  line-height: 75rpx;
  text-align: center;
  overflow: hidden;
}
.yesGroupbig{
  width: 100%;
  height: 0;
  position: fixed;
  z-index: 5000;
  top: 0;
  left: 0;
  overflow: hidden;
}
.yesGroupbig1{
  height: 0;
  transition: height 1s;
  -webkit-transition: height 1s;
  background: rgba(0,0,0,0.3);
}
.yesGroupbig2{
  height: 100%;
  transition: height 1s;
  -webkit-transition: height 1s;
  background: rgba(0,0,0,0.3);
}
.yesGroupbig view{
  height: 725rpx;
  width: 100%;
  position: relative;
}
.yesGroupbig view image{
  width: 100%;
  height: 725rpx;
}
.yesGroupbig view .groupNumber{
  position: absolute;
  top: 55rpx;
  left: 0;
  color: #595959;
  font-size: 24rpx;
  text-align: center;
  display: block;
  width: 100%;
}
.yesGroupbig view .groupCount{
  position: absolute;
  top: 310rpx;
  left: 0;
  color: #ffffff;
  font-size: 30rpx;
  text-align: center;
  display: block;
  width: 100%;
  letter-spacing: 3rpx;
}
.yesGroupbig view .time{
  position: absolute;
  top: 415rpx;
  left: 0;
  color: #ffffff;
  font-size: 24rpx;
  text-align: center;
  display: block;
  width: 100%;
}
.yesGroupbig view view{
  overflow: hidden;
  position: absolute;
  bottom: 95rpx;
  left: 0;
  height: 75rpx;
  width: 100%;
}
.yesGroupbig view view button{
  width: 225rpx;
  height: 75rpx;
  background: #cc171d;
  font-size: 24rpx;
  line-height: 75rpx;
  border-radius: 35rpx;
  float: left;
  color: #fff;
}
.yesGroupbig view view button:nth-of-type(1){
  margin-left: 130rpx;
}
.yesGroupbig view view button:nth-of-type(2){
  margin-left: 40rpx;
}
.yesGroupbig view .rule{
  position: absolute;
  bottom: 40rpx;
  left: 0;
  color: #ffffff;
  font-size: 24rpx;
  text-align: center;
  display: block;
  width: 100%;
}
.yesGroupbig view .bgImg{
  width: 92rpx;
  height: 36rpx;
  line-height: 30rpx;
  position: absolute;
  top: 150rpx;
  left: 50%;
  margin-left: -46rpx;
  background: url("https://img3.cloudokids.cn/h5img/scene/groupImg3.png") no-repeat;
  background-size: 100% 100%;
  font-size: 18rpx;
  text-align: center;
  color: #fff;
}
.yesGroupbig view .balanceMoney{
  position: absolute;
  top: 180rpx;
  left: 0;
  color: #ffffff;
  font-size: 100rpx;
  text-align: center;
  display: block;
  width: 100%;
  height: 120rpx;
}
.yesGroupbig view .balanceMoney text{
  font-size: 24rpx;
}
.groupTips{
