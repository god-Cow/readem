.tipsDel{
  position: fixed;
  width: 40rpx;
  height: 40rpx;
  background: url("https://img1.cloudokids.cn/buyers/scene/groupBooking3.png") no-repeat center center;
  background-size: 40rpx 40rpx;
  top: 50%;
  left: 50%;
  margin-top: -470rpx;
  margin-left: 270rpx;
  z-index: 400;
}
.pictureShare{
  display: none;
}
.top{
  top: 75rpx;
}
.rankContentTop{
  top: 155rpx;
}
.allWrap{
  z-index: 5000;
}
.specialBanner{
  margin-top: 75rpx;
}
.yesGroupbig view .newSkipUrl{
  position: absolute;
  top: 460rpx;
  left: 0;
  font-size: 36rpx;
  text-align: center;
  display: block;
  width: 100%;
  line-height: 36rpx;
  color: #cc171d;
}
._top{
