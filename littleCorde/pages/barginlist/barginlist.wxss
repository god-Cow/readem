.content{
  width: 100%;
  overflow: hidden;
}
.barginsize{
  width: 100%;
  height: 194rpx;
  border-top: 1px solid #dcdcdc;
  border-bottom: 1px solid #dcdcdc;
}
.bargintext {
  width: 100%;
  text-align: center;
  font-size: 30rpx;
  font-weight: lighter;
  margin: 60rpx auto 24rpx;
}

.bargintext text {
  color: #f44336;
}
.participation{
  font-size: 24rpx;
  color: #999999;
  text-align: center;
  margin: 0 auto;
}
page {
  -webkit-overflow-scrolling: touch;
  height: auto !important;
  background-color: #f5f5f5;
}
.lists{
  width: 100%;
  margin: 0 auto;
  overflow: hidden;
}
.list{
  width: 690rpx;
  height: 118rpx;
  margin: 0 auto;
    border-bottom: 1px solid #dcdcdc;
}
.userimg{
  width: 58rpx;
  height: 58rpx;
  margin-top: 30rpx;
  border-radius: 50%;
  display:block;
  float: left;
}
.username{
  line-height: 118rpx;
  font-size: 24rpx;
  color: #010101;
  display: block;
  height: 118rpx;
   float: left;
     margin-left: 20rpx;
}
.price{
  float: right;
  font-size: 24rpx;
  color: #999999;
  line-height: 118rpx;
}