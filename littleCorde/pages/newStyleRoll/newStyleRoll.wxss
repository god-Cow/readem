@import "/assets/css/size.wxss";
page{
  font-size: 26rpx; color: #333;
}
.viewWindow{ width: 100vw; height: 100vh; overflow:hidden;padding-bottom:100rpx;box-sizing: border-box;}
.viewWindow.Box2{padding-top: 40px;}
.viewWindow.Box3{padding-top:80px;}
.color_tint{color: #888; font-size: 24rpx;margin-left: 20rpx}
view,text{box-sizing: border-box}
.infoTop{
  line-height: 40px;
  border-bottom: 1px solid #e7e7e7;
  padding: 0 24px;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
  width: 100%;
  background: #fff; box-sizing: border-box;
}
.groupItem{ border-bottom: 10rpx solid #efefef;background: #fff; overflow: hidden;}
.groupItem .listLine{
  height: 140rpx;
  overflow: hidden; margin: 0 20rpx; display: -webkit-box; border-top:1px solid #e7e7e7;
}
.brandIng .lineTab{
  display: flex;
  display: -webkit-flex;
  align-items: center;
  text-align: left;
}
.brandIng .lineTab .person{color: #d0021b;}
.brandIng .lineTab:nth-of-type(1){ width: 50%; overflow: hidden; }
.brandIng .lineTab:nth-of-type(1)> view{
  margin-right: 10rpx;
}
.brandIng .lineTab:nth-of-type(2){
  font-size: 24rpx;
  flex-direction:column;
  justify-content:center;
  width: 25%;
}
.brandIng .lineTab:nth-of-type(2) text{color: #ff0000}
.groupItem .listLine .header{
  width: 96rpx; height: 96rpx; border-radius: 50%; overflow: hidden;
}
.groupItem .listLine .name{text-align: left; width: auto;}
.groupItem .listLine .header image{width: 100%; height: 100%;}
.brandIng .lineTab:nth-of-type(3){
  width: 25%; 
  justify-content: flex-end;
}
.brandIng .lineTab:nth-of-type(3) text{line-height: 64rpx;color: #fff; padding: 0 20rpx; border-radius: 32rpx;background: #535353; font-size: 24rpx;}
.groupItem .groupRule{
  display: flex;
  display: -webkit-flex; justify-content:space-between; height: 86rpx; line-height: 86rpx;
  border-top: 1px solid #e7e7e7; padding: 0 30rpx; margin-top: -1px;
}

.viewBrand{
  position: fixed; z-index:40; top:40px;transform: translateY(-.5px); width: 100%; overflow-y: scroll; background: rgba(0, 0, 0, 0.6);
}
.viewCarList{
  position: fixed; z-index:28; top:0;transform: translateY(-.5px); width: 100%; height: 100%; box-sizing: border-box; overflow-y: scroll; background: rgba(0, 0, 0, 0.6); padding-top:40%; padding-bottom:96rpx; display: none;
}
.viewCarList .carWrap{
  background: #fff;
}
.viewBrandInfo{
  position: fixed; z-index:30; top:35px;transform: translateY(-.5px); width: 100%; overflow-y: scroll; background: rgba(0, 0, 0, 0.3);
}
.brandWe{
  position: fixed; z-index:30; top:0;transform: translateY(-.5px); width: 100%;

}
.brandWeTitle{width: 100%; height:40px; line-height:40px;
  background: #f8f8f8; text-align: center; position: fixed; top: 0; z-index: 40;color: #f24b4b;font-size: 24rpx;
  border-bottom: 1px solid #e7e7e7;
}
.brandWe .listLine{height: 130rpx; display: flex; display:-webkit-flex;
  justify-content:space-between;align-items: center; overflow: hidden; background: #fff; padding: 0 20rpx;
  border-bottom: 1px solid #e7e7e7;
}
.brandWe .listLine .lineTab:nth-of-type(1){
  width: 200rpx; display:-webkit-box;
}
.brandWe .listLine .lineTab:nth-of-type(2){
  width: 410rpx;
  text-align: left;
}
.brandWe .listLine .lineTab .colorRed{color: #ff3737;}
.brandWe .listLine .lineTab:nth-of-type(3){
  width: 80rpx; text-align:right; position:relative;
}
.brandWe .listLine:not(.more) .lineTab:nth-of-type(3)::before{
  content: ''; border-right:1px solid #b1b1b1;border-top:1px solid #b1b1b1; position: absolute; right: 0; width: 15rpx; height: 15rpx;
  transform: rotate(45deg); top:50%;
}
.brandWe .header {
  width: 80rpx; height: 80rpx; border-radius: 50%; border:2px solid #ffcfc6;
  padding: 2px; overflow: hidden;
}
.brandWe .header image{width: 100%;height: 100%; border-radius: 50%;}

.filtrateWrap{width: 100%; height: 44px;}
.goodsList .filtrate{
