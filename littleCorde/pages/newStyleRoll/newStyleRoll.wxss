.addView{ width:20%; height:100%;position: absolute; right: 0; bottom:0;}
.cartList .addView{
  width: 15%;
}
.addView .addBtn{
  width: 60rpx; height: 60rpx; line-height: 60rpx;
  text-align: center;  background: #535353; font-size: 40rpx; font-weight:bold; color:#fff; border-radius: 75%; position: absolute; bottom: 40rpx; right: 0;
}
.addView .letter{position: absolute;top:50%;left: 0; text-align: center; transform: translateY(-50%);width: 100%;}
.addBtn::before,.addBtn::after{ 
  content: ''; 
  position: absolute; top:50%; left: 50%;   transform: translateX(-50%) translateY(-50%);
  background:#fff;
}
.addBtn::before {width:24rpx; height:2px;}
.addBtn::after{ 
  width:2px; height:24rpx;
}
.bagBarItem{position: fixed; bottom:0; z-index:33; height:98rpx;border-top: 1px solid #dcdcdc;
  background: #fff; width: 100%; justify-content: space-between; display: table; vertical-align: middle;padding-left: 30rpx;
}
.bagBarItem>view:first-child{line-height:98rpx;}
