.group{
  border-bottom: 1px solid #dcdcdc
}
.list{
  overflow: hidden;
  width: 100%;
  height: 530rpx;
}
.header{
  height: 80rpx;
  background: #eeeeee;
  width: 100%;
  font-size: 24rpx;
  color: #010101;
  line-height: 80rpx;
}
.header text:nth-of-type(1){
  float: left;
  margin-left: 30rpx;
}
.header text:nth-of-type(2){
  float: right;
  margin-right: 30rpx;
}
.groupInfo{
  width: 100%;
  height: 450rpx;
  position: relative;
  overflow: hidden;
}
.groupInfo .info{
  position: absolute;
  width: 430rpx;
  height: 190rpx;
  background: rgba(0,0,0,0.5);
  z-index:100;
  bottom: 0;
  left: 50%;
  margin-left: -215rpx;
}
.text{
  width: 100%;
  text-align: center;
  font-size: 30rpx;
  line-height: 55rpx;
  color: #fff;
}
.tips{
  text-align: center;
  color: #ffffff;
  font-size: 24rpx;
  line-height: 50rpx;
  padding-bottom: 20rpx;
}
.textTips{
  font-size: 24rpx;
  color: #010101;
  text-align: center;
  line-height: 200rpx;
}
.groupInfo .groupBtn{
  width: 100%;
  overflow: hidden;
}
.groupBtn button{
  width: 130rpx;
  height: 40rpx;
  line-height: 40rpx;
  background: #ffffff;
  color: #010101;
  font-size: 24rpx;
  float: left;
}
.groupBtn button:nth-of-type(1){
  margin-left: 70rpx;
}
.groupBtn button:nth-of-type(2){
  margin-left: 30rpx;
}
.on{  
  border-bottom: 1rpx solid #000;
}
.swiper-tab{ 
  width: 100%; 
  border-bottom: 10px solid #f3f3f3;
  border-top: 1px solid #eaeaea; 
  text-align: center; 
  line-height: 80rpx;
  } 
.swiper-tab-item{ 
  font-size: 30rpx; 
  display: inline-block; 
  width: 33.3%; 
  color: #666666; 
}
.swiper-tab-item text{
  display: block;
}
.swiper-tab-item image{
  width: 50px;
  height: 50px;
}