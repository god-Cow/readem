.weui_cell{
    position: relative;
    display: flex;
    padding: 10px;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    border-bottom: 1px solid #eaeaea;
    background: #ffffff;
}
.weui_cell_hd{
  width: 25px;
  margin-right: 20px;
}
.new_bd{
  flex:1;
}
.weui_cell_hd image{
  width: 100%;
  height: 25px;
  vertical-align: -2px;
}
.weui_cell_bd_p{
  font-size: 14px;
}
.weui_cell_new{
  height: 20px; 
  justify-content: right;
  text-align: right;
  float: right;
}
.weui_cell_new image{
  width: 12px;
  height: 20px;
  margin-top: 1px;
}
.allOrder{
  font-size: 14px;
}
.orderStyle{
