.personCenter{
  width: 220rpx;
  height: 50rpx;
  background: #ccc;
  float: right;
  margin-top: 89rpx;
  color: #fff;
  border-top-left-radius: 30rpx;
  border-bottom-left-radius: 30rpx;
  font-size: 12px;
  line-height: 50rpx;
  background-image: url("http://img1.cloudokids.cn/buyers/scene/baisesanjiao.png");
  background-repeat: no-repeat;
  background-position: 95% center;
  background-size: 14rpx 20rpx;
}
