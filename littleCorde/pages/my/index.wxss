.serviceLogo{
  position: relative;
}
.service{
  position: absolute;
  top: 0;
  left: 0;
  font-size: 24rpx;
  background: url("http://img1.cloudokids.cn/buyers/scene/kefu.png") no-repeat center 40rpx;
  background-size: 43rpx 38.9rpx;
  display: block;
  width:100%;
  height: 180rpx;
  text-align: center;
  line-height: 115px;
  color: #575451;
  border-radius: 0!important;
  border: 0!important;
  padding: 0!important;
  margin: 0!important;
}
button::after{
  content: none;
}
.group{
