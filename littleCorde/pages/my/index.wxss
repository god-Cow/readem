}
.userinfo{
  background: #f5f5f5;
  height: 228rpx;
}
.avatarWrap{
  height:228rpx;
}
.classname{
  float: left;
  height: 228rpx;
}
.avatarWrap{
  margin-left: 30px;
}
.nickName{
  line-height: 228rpx;
  margin-left: 20rpx;
}
.userinfo .classname .userinfo-avatar {
  display: block;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  margin: 54rpx auto;
  text-align: center;
}
