.trumpet{
  height: 60rpx;
  line-height: 60rpx;
  font-size: 24rpx;
  color: #fff;
  background: #ccc url("http://img1.cloudokids.cn/buyers/scene/trumpet.png") no-repeat 15rpx 15rpx;
  background-size: 30rpx 30rpx;
  padding-left: 50rpx;
  overflow: hidden;
}
.trumpet text{
  display: block;
  float: left;
}
.trumpet button{
  color: #f1989b;
  border: 1px solid #f1989b;
  float: left;
  height: 40rpx;
  border-radius: 0;
  line-height: 37rpx;
  font-size: 24rpx;
  margin-top: 10rpx;
  margin-left: 20rpx;
  width: 125rpx;
  padding-left: 0;
  padding-right: 0;
}
.trumpet navigator{
  text-align: center;
  color: #f1989b;
  border: 1px solid #f1989b;
  float: left;
  height: 40rpx;
  border-radius: 0;
  line-height: 37rpx;
  font-size: 24rpx;
  margin-top: 10rpx;
  margin-left: 20rpx;
  width: 125rpx;
  padding-left: 0;
  padding-right: 0;
  background-color: #fff;
}
.trumpet button::after{
  content: none
}
.phoneCode{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  background: rgba(0,0,0,0.3);
}
.phoneCodeInfo{
  width: 550rpx;
  height: 350rpx;
  background: #fff;
  position: fixed;
  top: 50%;
  left: 50%;
  margin-top: -175rpx;
  margin-left: -275rpx;
  border-radius: 15rpx;
}
.phoneCodeInfo view{
  text-align: center;
}
.del{
  width: 100%;
  height: 40rpx;
  background: url("https://img1.cloudokids.cn/buyers/scene/groupBooking3.png") no-repeat top right 20rpx;
  background-size: 40rpx 40rpx;
  margin-top: 20rpx;
}
.phoneCodeInfo .text1{
  font-size: 28rpx;
  line-height: 40rpx;
  margin-top: 20rpx;
}
.phoneCodeInfo .text2{
  font-size: 24rpx;
  line-height: 40rpx;
  color: #ccc;
}
.phoneCodeInfo .code{
  border: 1px solid #ccc;
  border-radius: 0;
  width: 400rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 24rpx;
  margin: 30rpx auto;
  padding: 0 10rpx;
  text-align: center;
}
.submitInfo{
  width: 120rpx;
  height: 50rpx;
  line-height: 50rpx;
  background: #010101;
  color: #fff;
  text-align: center;
  font-size: 24rpx;
  border:0;
  border-radius: 0;
}
.submitInfo::after{
  content: none
}
