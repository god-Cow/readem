page{
  height: 100%;
  box-sizing: border-box;
}
.container{
  width:100%;
  height: 100%;
}
.flex .item_left{
  display: block;
  width:100%;
}
.topHeader{
  height:30px;
  width:100%;
  display: block;
  background: rgba(239, 239, 244, 1);
  line-height: 30px;
  font-size: 16px;
  padding-left: 15px;
  color: #666;
}
.brandName{
  height: 80rpx;
  line-height: 80rpx;
  border-top: 1px solid #eaeaea;
  margin: 0 20px 0 15px;
}
.item_left .brandName:nth-of-type(1){
  border-top:0;
}
.nextData{
  display: block;
  font-size: 24rpx;
  color: #010101;
  font-family: "Avenir Next";
}
.isTopBrand scroll-view{
  width: 100%;
  white-space: nowrap;
}
.isTopBrand .isTopBrandName{
  display: inline-block;
  height: 160rpx;
  width: 160rpx;
  margin: 0 10rpx;
}
.isTopBrand .isTopBrandName navigator image{
  height: 160rpx;
  width: 160rpx;
}
.orientation_region{width: 30rpx;font-size: 22rpx;position: fixed;right: 10rpx;font-weight: bolder;height:720rpx;top: 50%;margin-top: -360rpx} 
.orientation_city{height: 30rpx; line-height: 30rpx;color: #000;text-align: center;} 
