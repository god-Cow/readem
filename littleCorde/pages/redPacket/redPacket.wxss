page{
  background: #f0f0f0;
}
.content{
  width: 100%;
}
.banner{
  width: 100%;
  height: 740rpx;
}
.banner image{
  width: 100%;
  height: 100%;
}
.express{
  background: #fff;
}
.express .text{
  padding-top: 30rpx;
  padding-bottom: 40rpx;
  width: 100%;
  text-align: center;
  font-size: 30rpx;
  color: #666;
}
.express .text text{
  color: #D0021B;
}
.line{
  background: url('https://img3.cloudokids.cn/h5img/specialSale/getCoupon46.png') no-repeat;
  width: 48rpx;
  height: 12rpx;
  background-size:100% 100%; 
  float: left;
  margin-left: 28rpx;
  margin-right: 28rpx;
  margin-top: 42rpx;
}
.expressInfo{
  float: left;
  width: 120rpx;
}
.redPacketImg .img{
  border-radius: 50%;
  width: 96rpx;
  height: 96rpx;
  border: 4rpx solid #fdd1c8;
  margin: 0 auto;
}
.redPacketImg .newImg{
  border-radius: 50%;
  width: 96rpx;
  height: 96rpx;
  border: 4rpx solid #eeeeee;
  margin: 0 auto;
}
.redPacketImg .img image{
  width: 96rpx;
  height: 96rpx;
  border-radius: 50%;
}
.redPacketImg .newImg image{
  width: 96rpx;
  height: 96rpx;
  border-radius: 50%;
}
.redPacketImg{
  width: 120rpx;
}
.name{
  font-size: 24rpx;
  color: #666;
  width: 95%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-align: center;
  margin: 15rpx auto;
  display: block;
}
.btn{
  width: 100%;
  height: 80rpx;
  background: #fff;
  margin-top: 40rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}
.btn .share{
  width: 510rpx;
  height: 80rpx;
  background: #000;
  color: #fff;
  font-size: 30rpx;
  border-radius: 50rpx;
  line-height: 80rpx;
}
.twoBtn{
  width: 550rpx;
  margin: 0 auto;
}
.twoBtn button{
  width: 264rpx;
  height: 80rpx;
  border-radius: 50rpx;
  font-size: 30rpx;
  float: left;
  color: #fff;
  background: #000;
}
.twoBtn button:nth-child(2){
  float: right;
  background: #ccc;
}
.gameOver{
  text-align: center;
  line-height: 600rpx;
  font-size: 26rpx;
}