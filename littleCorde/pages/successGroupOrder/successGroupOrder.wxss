.content{
  width: 100%;
  position: relative;
}
.content image{
  width: 400rpx;
  height: 400rpx;
  margin: 60rpx auto 0;
  display: block;
}
.brandName{
  font-size: 24rpx;
  line-height: 36rpx;
  text-align: center;
  letter-spacing: 2rpx;
}
.goodsName{
  font-size: 20rpx;
  line-height: 36rpx;
  text-align: center;
  letter-spacing: 2rpx;
}
.price{
  font-size: 28rpx;
  line-height: 36rpx;
  display: flex;
}
.price>view{
  flex: 2;
}
.price>view:nth-of-type(1){
  text-align: right;
  margin-right: 10rpx;
}
.price>view:nth-of-type(2){
  text-align: left;
  margin-left: 10rpx;
}
.price>view>text{
  font-size: 20rpx;
  color: #666666;
}
.link{
  height: 30rpx;
  width: 100rpx;
  border: 1px solid #666666;
  font-size: 18rpx;
  line-height: 30rpx;
  color: #999999;
  text-align: center;
  position: absolute;
  top: 60rpx;
  right: 50rpx;
}
.groupInfo{
  padding-bottom: 60rpx;
}
.groupInfo .number{
  width: 600rpx;
  height: 75rpx;
  background: #fff7f2;
  text-align: center;
  font-size: 24rpx;
  line-height: 75rpx;
  margin: 40rpx auto;
}
.time{
  text-align: center;
  font-size: 20rpx;
  line-height: 36rpx;
}
.blackPatch{
  display: inline-block;
  width: 30rpx;
  height: 30rpx;
  color: #fff;
  background: #010101;
  line-height: 30rpx;
  text-align: center;
  border-radius: 5rpx;
}
.shareBtn{
  width: 590rpx;
  height: 80rpx;
  text-align: center;
  background: #010101;
  color: #fff;
  font-size: 30rpx;
  line-height: 80rpx;
  margin-top: 60rpx;
}
.shareBtn1{
  background: #535353;
  margin-top: 20rpx;
}
.flow{
  color: #999999;
  font-size: 18rpx;
  line-height: 24rpx;
  text-align: center;
  margin: 60rpx auto 80rpx;
}
.btn{
  overflow: hidden;
}
.btn button{
  float: left;
  width: 180rpx;
  height: 60rpx;
  border: 1px solid #666666;
  font-size: 24rpx;
}
.btn button:nth-of-type(1){
  margin-left: 175rpx;
}
.btn button:nth-of-type(2){
  margin-left: 40rpx;
}
.canvasBg{
  position: fixed;
  height: 100%;
  width: 100%;
  left: 0;
  top: 0;
  z-index: 100;
  background: rgba(0,0,0,0.3);
}
.spuCanvas{
  position: fixed;
  width: 280px;
  height: 480px;
  left: 50%;
  top: 50%;
  z-index: 111;
  margin-top: -240px;
  margin-left: -140px;
}
.spuCanvasImg{
  width: 280px;
  height: 400px;
}
.spuCanvas view{
  height: 80px;
  width: 280px;
  overflow: hidden;
}
.spuCanvas view button{
  border: 0;
  border-radius: 0;
  height: 40px;
  width: 220rpx;
  font-size: 24rpx;
  line-height: 40px;
  margin-top: 20px;
}
.spuCanvas view button::after{
  content: none
}
.spuCanvas view .btn1{
  background: #010101;
  color: #fff;
  float: left;
}
.spuCanvas view .btn2{
  background: #666;
  color: #fff;
  float: right;
}